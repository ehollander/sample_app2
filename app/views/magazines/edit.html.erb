<% provide(:title, "Edit magazine") %>
<div> Logged in as: <%= current_user.name %></div> 

<h1>Update magazine</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@magazine) do |f| %>
    <%#= render 'shared/error_messages', object: f.object %>

      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :division %>
      <%= f.text_field :division %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
      
    <% end %>
  </div>
</div>

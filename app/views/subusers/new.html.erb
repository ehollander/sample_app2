<% provide(:title, "Add Sub Agency") %>
<h1>Add Sub Agency for <u><%= @user.name %></u></h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@subuser) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :subname %>
      <%= f.text_field :subname %>
   
      <%= f.hidden_field :user_id, :value => params[:user_id] %>

      <%= f.submit "Create Sub Agency", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>